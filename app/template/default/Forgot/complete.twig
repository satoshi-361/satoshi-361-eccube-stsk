{% extends 'default_frame.twig' %}
{% block stylesheet %}
    <link rel="stylesheet" href="{{ asset('assets/css/contents.css') }}" type="text/css">
    <link rel="stylesheet" href="{{ asset('assets/css/sp_top.css') }}" type="text/css">
    <link rel="stylesheet" href="{{ asset('assets/css/custom.css') }}" type="text/css">
{% endblock %}

{% block main %}
    <article id="main_wrap">
        <div id="breadcrumb">
            <ul>
                <li><a href="{{ url('homepage') }}}}">ホーム</a></li>
                <li>パスワード再設定</li>
            </ul>
        </div>
        <section class="section form">
        <div class="ec-role">
            <div class="ec-pageHeader">
                <h1>{{ 'front.forgot.complete_title'|trans }}</h1>
            </div>
        </div>
        <div class="ec-forgetCompleteRole">
            <div class="ec-off3Grid ec-text-ac">
                <div class="ec-off3Grid__cell">
                    <div class="ec-reportHeading">
                        <h2>{{ 'front.forgot.complete_message__title'|trans }}</h2>
                    </div>
                    <p class="ec-reportDescription">{{ 'front.forgot.complete_message__body'|trans }}</p>
                </div>
            </div>
        </div>
        </section>
    </article>
{% endblock %}


